<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- http://stackoverflow.com/questions/7568579/eclipsefindbugs-exclude-filter-files-doesnt-work -->
    <Match>
        <Or>
            <Class name="~.*\.R\$.*"/>
            <Class name="~.*\.Manifest\$.*"/>
            <Package name="~io\.realm\.*"/>
            <Bug pattern="SUA_SUSPICIOUS_UNINITIALIZED_ARRAY" />
        </Or>
    </Match>
</FindBugsFilter>
